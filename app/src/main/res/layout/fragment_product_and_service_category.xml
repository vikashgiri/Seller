<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <RelativeLayout
        android:id="@+id/list"
        android:background="@color/white"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >
           <RelativeLayout
            android:id="@+id/rel1"
            android:layout_below="@+id/toolbar"
            android:padding="@dimen/dp_10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="wrap_content"
                android:textSize="@dimen/sp_18"
                android:textStyle="bold"
                android:textColor="@color/dark_gray"
                android:layout_height="wrap_content"
                android:text="@string/category_name">

            </TextView>

            <TextView
                android:layout_marginRight="@dimen/dp_20"
                android:layout_width="wrap_content"
                android:textSize="@dimen/sp_18"
                android:textStyle="bold"
                android:layout_alignParentRight="true"
                android:textColor="@color/dark_gray"
                android:layout_height="wrap_content"
                android:text="@string/no_of_seller" />
        </RelativeLayout>
        <View
            android:id="@+id/view"
            android:layout_below="@+id/rel1"
            android:background="@color/colorPrimary"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_1"></View>
        <RelativeLayout
            android:id="@+id/rel2"
            android:paddingLeft="@dimen/dp_10"
            android:layout_margin="@dimen/dp_10"
            android:layout_below="@+id/view"
            android:background="@drawable/ic_rectangle_gray_cornor"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="wrap_content"
                android:textSize="@dimen/sp_14"
                android:textStyle="bold"
                android:layout_centerVertical="true"
                android:textColor="@color/dark_gray"
                android:layout_height="wrap_content"
                android:text="43304">
            </TextView>
            <Button
                android:id="@+id/item_number"
                android:background="@drawable/ic_button_background"
                android:layout_width="70dp"
                android:textSize="@dimen/sp_14"
                android:textStyle="bold"
                android:layout_alignParentRight="true"
                android:textColor="@color/white"
                android:layout_height="30dp"
                android:text=" 100">
            </Button>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rel3"
            android:layout_margin="@dimen/dp_10"
            android:background="@drawable/ic_rectangle_gray_cornor"

            android:layout_below="@+id/rel2"
            android:paddingLeft="@dimen/dp_10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="wrap_content"
                android:textSize="@dimen/sp_14"
                android:textStyle="bold"
                android:layout_centerVertical="true"

                android:textColor="@color/dark_gray"
                android:layout_height="wrap_content"
                android:text="@string/accounting_based_credit_product">

            </TextView>

            <Button
                android:id="@+id/esclate"
                android:layout_width="70dp"

                android:layout_height="30dp"
                android:layout_alignParentRight="true"
                android:background="@drawable/ic_button_background"
                android:text="20"
                android:textColor="@color/white"
                android:textSize="@dimen/sp_14"
                android:textStyle="bold">

            </Button>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/rel4"
            android:layout_margin="@dimen/dp_10"
            android:background="@drawable/ic_rectangle_gray_cornor"

            android:layout_below="@+id/rel3"
            android:paddingLeft="@dimen/dp_10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="wrap_content"
                android:textSize="@dimen/sp_14"
                android:textStyle="bold"
                android:layout_centerVertical="true"

                android:textColor="@color/dark_gray"
                android:layout_height="wrap_content"
                android:text="@string/acute_care_montitoring_unit">

            </TextView>

            <Button            android:background="@drawable/ic_button_background"

                android:layout_width="70dp"
                android:textSize="@dimen/sp_14"
                android:textStyle="bold"
                android:layout_alignParentRight="true"
                android:textColor="@color/white"
                android:layout_height="30dp"
                android:text=" 04">

            </Button>

        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/rel5"
            android:layout_margin="@dimen/dp_10"
            android:background="@drawable/ic_rectangle_gray_cornor"

            android:layout_below="@+id/rel4"
            android:paddingLeft="@dimen/dp_10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="wrap_content"
                android:textSize="@dimen/sp_14"
                android:textStyle="bold"
                android:layout_centerVertical="true"

                android:textColor="@color/dark_gray"
                android:layout_height="wrap_content"
                android:text="@string/adheservices">

            </TextView>

            <Button            android:background="@drawable/ic_button_background"

                android:layout_width="70dp"
                android:textSize="@dimen/sp_14"
                android:textStyle="bold"
                android:layout_alignParentRight="true"
                android:textColor="@color/white"
                android:layout_height="30dp"
                android:text="24">

            </Button>

        </RelativeLayout>
    <RelativeLayout
            android:layout_margin="@dimen/dp_10"
            android:background="@drawable/ic_rectangle_gray_cornor"

            android:layout_below="@+id/rel5"
            android:paddingLeft="@dimen/dp_10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="wrap_content"
                android:textSize="@dimen/sp_14"
                android:textStyle="bold"
                android:layout_centerVertical="true"

                android:textColor="@color/dark_gray"
                android:layout_height="wrap_content"
                android:text="@string/adrenergic_blocking_agent">

            </TextView>

            <Button            android:background="@drawable/ic_button_background"

                android:id="@+id/open"
                android:layout_width="70dp"
                android:textSize="@dimen/sp_14"
                android:textStyle="bold"
                android:layout_alignParentRight="true"
                android:textColor="@color/white"
                android:layout_height="30dp"
                android:text="204">

            </Button>

        </RelativeLayout>


    </RelativeLayout>
</layout>